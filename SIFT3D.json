{
	"actions": {
		"SIFT3D": {
			"parameters": [
				{
					"name": "inputVolume",
					"type": "file",
					"required": true
				},
				{
					"prefix": "-s ",
					"name": "spacing",
					"type": "float"
				},
				{
					"prefix": "-pt ",
					"name": "peakThreshold",
					"aliases" : [ "threshold" ],
